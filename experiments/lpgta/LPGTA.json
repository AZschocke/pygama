{
  "daq":"FlashCam",
  "daq_dir":"$LPGTADIR/daq",
  
  "tier_types":["raw","dsp","hit"],
  "raw_dir":"$LPGTADIR/raw",
  "dsp_dir":"$LPGTADIR/dsp",
  "hit_dir":"$LPGTADIR/hit",
  
  "meta_dir":"$LPGTADIR/metadata",
  "meta_types":["ged-ene","ged-aoe"],
  
  "daq_to_raw":{
    "buffer_size":8096,
    
    "daq_filename_template":"{YYYYmmdd}-{hhmmss}-{runtype}.fcio",
    "filename_info_mods":{
        "runtype":{
            "phys":"phy",
            "calib":"cal"
        }
    },
    "raw_filename_template":"{file_label}/{runtype}/LPGTA_r{run:0>4d}_{YYYYmmdd}T{hhmmss}Z_{runtype}_{file_label}_raw.lh5",
    
    "ch_groups":{
      "g{ch:0>3d}": {
        "ch_range":[24,64],
        "tb_per_ch":"True",
        "file_label":"geds"
      },
      "spms": {
        "ch_range":[6,23],
        "file_label":"spms"
      },
      "puls": {
        "ch_range":[0,0],
        "file_label":"auxs"
      },
      "muvt": {
        "ch_range":[1,5],
        "file_label":"auxs"
      }
    }
  },

  "raw_to_dsp":{
  }
}
